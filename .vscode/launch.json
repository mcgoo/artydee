    "version": "0.2.0",
    "configurations": [
        {
            "name": "run regsvr32 (32 bit)",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "regsvr32",
            "args": ["/s", "target\\i686-pc-windows-msvc\\debug\\muppet.dll"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [{ "name": "RUST_LOG", "value": "debug" }],
            "externalConsole": false
        },   
        {
            "name": "run excel (32 bit)",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "C:\\Program Files (x86)\\Microsoft Office\\root\\Office16\\EXCEL.EXE",
            "args": ["sheets\\rtdtest.xlsm"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [{ "name": "RUST_LOG", "value": "debug" }],
            "externalConsole": false
        }
    ]
}